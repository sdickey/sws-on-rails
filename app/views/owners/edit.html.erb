<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2 well well-lg">
      <div class="well well-lg" style="background: #fafafa;">
        <h1 class="form-header">Edit Owner</h1>
      </div>
      <%= form_for ([@account, @account.owners.build]), url: account_owner_path(@owner.account_id, @owner.id), method: :put do |f| %>

        <p>
          <%= f.submit :value => "Update owner", :class => "btn btn-primary" %>
          <%= link_to 'Back', account_dashboard_path(@owner.account_id), :class => 'btn btn-primary' %>
        </p>
      <% end %>
    </div>
  </div>
</div>